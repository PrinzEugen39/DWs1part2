    <!-- NAV -->
    <nav class="navbar navbar-expand-lg bg-danger-subtle position-sticky top-0">
      <div class="container ">
          <a class="navbar-brand" href="/"><img src="/image/logo.png" alt="Dumbways" /></a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
              data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
              aria-label="Toggle navigation">
              <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
              <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                  <li class="nav-item">
                      <a class="nav-link active" aria-current="page" href="/">Home</a>
                  </li>
                  <li class="nav-item">
                      <a class="nav-link" href="/blog">Add Project</a>
                  </li>
              </ul>
              <form class="d-flex" role="search">
                  <a class="btn btn-dark" href="/contact">Contact Me</a>
              </form>
          </div>
      </div>
  </nav>


    <!-- ProjectDetail -->
    <div class="container my-5">
      <div class="blog-detail-container">
        <h1>
          {{blog.title}}
        </h1>
        <div class="author">Duration: {{blog.duration}} | PrinzEugen39</div>
        <div class="my-3">
          <img class="w-50" src="/image/Tianasquare.jpg" alt="detail" />
        </div>
        <p>
          {{blog.content}}
        </p>
      </div>
    </div>
    <script src="js/navbar.js"></script>
